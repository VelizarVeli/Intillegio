@model IEnumerable<Intillegio.Common.ViewModels.QuickLinksViewModel>
<div class="col-md-3 col-sm-6 col-12">
    <h3>Quick Links</h3>
    <ul class="footer-list">

        @foreach (var quiLink in Model)
        {
            <li><a target="_blank" href="@quiLink.Link">@quiLink.Name</a></li>
        }
    </ul>
</div>