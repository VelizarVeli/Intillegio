@model IEnumerable<Intillegio.Common.ViewModels.PartnerViewModel>
<div class="owl-carousel owl-theme clients" id="clients">
    @foreach (var client in Model)
    {
      <div class="item">
          <a href="@client.LinkToPartnersWebsite" target="_blank"><img src="@client.Logo155X75" alt="partner-image"></a>
      </div>
    }
</div>