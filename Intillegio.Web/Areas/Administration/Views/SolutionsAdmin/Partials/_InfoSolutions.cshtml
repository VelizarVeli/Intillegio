<section id="posts">
    <div class="container">
        <div class="row">
            <div class="col-md-9">
                <div class="card">
                    <div class="card-header">
                        <h4>Services</h4>
                    </div>
                    <table class="table table-striped">
                        <thead class="thead-inverse">
                        <tr>
                            <th>#</th>
                            <th>Name</th>
                        </tr>
                        </thead>
                        <tbody>

                        @foreach (var service in Model)
                        {
                            <tr>
                                <td scope="row">@service.Id</td>
                                <td>@service.Name</td>
                                <td>
                                    <a asp-area="Administration" asp-controller="SolutionsAdmin" asp-action="SolutionDetails" asp-route-id="@service.Id" class="btn btn-secondary">
                                        <i class="fa fa-angle-double-right"></i> Details
                                    </a>
                                </td>
                            </tr>
                        }
                        </tbody>
                    </table>
                </div>
            </div>
            <partial name="~/Areas/Administration/Shared/_Users.cshtml"/>
        </div>
    </div>
</section>